      <div class="jumbotron">
        <p><%= info.nodeVersion %></p>
      </div>
      <h2>Memoria total reservada</h2>
      <div class="jumbotron">
        <p><%= info.rss %></p>
      </div>
      <h2>Path de ejecuci√≥n</h2>
      <div class="jumbotron">
        <p><%= info.execPath %></p>
      </div>
